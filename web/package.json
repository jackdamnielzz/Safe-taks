{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:update-snapshots": "jest --updateSnapshot",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:headed": "cypress run --headed",
    "test:all": "npm run test:ci && npm run e2e",
    "emulators": "firebase emulators:start",
    "emulators:dev": "firebase emulators:start --only firestore,auth,storage",
    "emulators:test": "firebase emulators:exec --only firestore,auth,storage \"npm run test\"",
    "emulators:e2e": "firebase emulators:exec --only firestore,auth,storage \"npm run e2e\"",
    "emulators:kill": "firebase emulators:kill",
    "test:emulated": "firebase emulators:exec --only firestore,auth,storage \"npm run test:ci\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/nextjs": "^10.17.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "browser-image-compression": "^2.0.2",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "idb": "^8.0.3",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.545.0",
    "micro": "^10.0.1",
    "negotiator": "^1.0.0",
    "next": "15.5.4",
    "next-intl": "^4.3.9",
    "next-pwa": "^5.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "recharts": "^3.2.1",
    "resend": "^6.1.2",
    "stripe": "^18.5.0",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.6",
    "@eslint/eslintrc": "^3",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@jest/globals": "^30.2.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^15.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "firebase-tools": "^14.17.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "lint-staged": "^16.2.3",
    "nyc": "^17.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.4.4",
    "typescript": "^5"
  }
}
